    },
    {
        "id": 64,
        "arabic": "كَأَنَّ بِالنَّارِ مَا بِالْمَاءِ مِنْ بَلَلٍ ",
        "arabic1":"حُزْنًا وَبِالْمَاءِ مَا بِالنَّارِ مِنْ ضَرَمِ"
    },
    {
        "id": 65,
        "arabic": " وَالْجِنُّ تَهْتِفُ وَالْأَنْوَارُ سَاطِعَةٌ ",
        "arabic1":"وَالْحَقُّ يَظْهَرُ مِنْ مَعْنًى وَمِنْ كَلِمِ"
    },
    {
        "id": 66,
