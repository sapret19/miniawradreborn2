    {
        "id": 88,
        "arabic": "دَعْنِي وَوَصْفِيَ آيَاتٍ لَهُ ظَهَرَتْ ",
        "arabic1":"ظُهُوْرَ نَارِ الْقِرَى لَيْلًا عَلَى عَلَمِ"
    },
    {
        "id": 89,
        "arabic": "فَالدُّرُّ يَزْدَادُ حُسْنًا وَهُوَ مُنْتَظِمٌ ",
        "arabic1":"وَلَيْسَ يَنْقُصُ قَدْرًا غَيْرَ مُنْتَظِمِ"
    },
    {
        "id": 90,
        "arabic": "فَمَا تَطَاوُلُ آمَالِ الْمَدِيْحِ إِلَى ",
        "arabic1":"مَا فِيْهِ مِنْ كَرَمِ الْأَخْلَاقِ وَالشِّيَمِ"
    },
    {
        "id": 91,
        "arabic": "آيَاتُ حَقٍّ مِنَ الرَّحْمَنِ مُحْدَثَةٌ ",
        "arabic1":" قَدِيْمَةٌ صِفَةُ الْمَوْصُوْفِ بِالْقِدَمِ"
    },
    {
        "id": 92,
        "arabic": "لَمْ تَقْتَرِنْ بِزَمَانٍ وَهِيَ تُخْبِرُنَا ",
        "arabic1":"عَنِ الْمَعَادِ وَعَنْ عَاٍد وَعَنْ إِرَمِ"
    },
    {
        "id": 93,
        "arabic": " دَامَتْ لَدَيْنَا فَفَاقَتْ كُلَّ مُعْجِزَةٍ ",
        "arabic1":"مِنَ النَّبِيِّيْنَ إِذْ جَاءَتْ وَلَمْ تَدُمِ"
    },
    {
        "id": 94,
        "arabic": "مُحَكَّمَاتٌ فَمَا تُبْقِيْنَ مِنْ شُبَهٍ ",
        "arabic1":"لِذِي شِقَاقٍ وَمَا تَبْغِيْنَ مِنْ حَكَمِ"
    },
    {
        "id": 95,
        "arabic": "مَا حُوْرِبَتْ قَطُّ إِلَّا عَادَ مِنْ حَرَبٍ ",
        
        "arabic1":"أَعْدَى الْأَعَادِي إِلَيْهَا مُلْقِيَ السَّلَمِ"
    },
    {
        "id": 96,
        "arabic": " رَدَّتْ بَلَاغَتُهَا دَعْوَى مُعَارِضِهَا ",
        "arabic1":"رَدَّ الْغَيُوْرِ يَدَ الْجَانِي عَنِ الْحَرَمِ"
    },
    {
        "id": 97,
        "arabic": "لَهَا مَعَانٍ كَمَوْجِ الْبَحْرِ فِي مَدَدٍ ",
        "arabic1":"وَفَوْقَ جَوْهَرِهِ فِي الْحُسْنِ وَالْقِيَمِ"
    },
    {
        "id": 98,
        "arabic": "فَمَا تُعَدُّ وَلَا تُحْصَى عَجَائِبُهَا ",
        "arabic1":"وَلَا تُسَامُ عَلَى الْإِكْثَارِ بِالسَّأَمِ"
    },
    {
        "id": 99,
        "arabic": "قَرَّتْ بِهَا عَيْنُ قَارِيْهَا فَقُلْتُ لَهُ ",
        "arabic1":"لَقَدْ ظَفِرْتَ بِحَبْلِ اللَّهِ فَاعْتَصِمِ"
    },
    {
        "id": 100,
        "arabic": "إِنْ تَتْلُهَا خِيْفَةً مِنْ حَرِّ نَارِ لَظَى ",
        "arabic1":"أَطْفَأْتَ حَرَّ لَظَى مِنْ وِرْدِهَا الشِّيَمِ"
    },
    {
        "id": 101,
        "arabic": "كَأَنَّهَا الْحَوْضُ تَبْيَضُّ الْوُجُوْهُ بِهِ ",
        "arabic1":"مِنَ الْعُصَاةِ وَقَدْ جَائُوْهُ كَالْحُمَمِ"
    },
    {
        "id": 102,
        "arabic": "وَكَالصِّرَاطِ وَكَالْمِيْزَانِ مَعْدَلَةً ",
        "arabic1":"فَالْقِسْطُ مِنْ غَيْرِهَا فِي النَّاسِ لَمْ يَقُمِ"
    },
    {
        "id": 103,
        "arabic": " لَا تَعْجَبَنْ لِحَسُوْدٍ رَاحَ يُنْكِرُهَا ",
        "arabic1":"تَجَاهُلًا وَهُوَ عَيْنُ الْحَاذِقِ الْفَهِمِ"
    },
    {
        "id": 104,
