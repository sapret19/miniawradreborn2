    {
        "id": 152,
        "arabic": "يَا أَكْرَم َالْخَلْقِ مَالِي مَنْ أَلُوْذُ بِهِ",
        "arabic1":" سِوَاكَ عِنْدَ حُلُوْلِ الْحَادِثِ الْعَمَمِ"
    },
    {
        "id": 153,
        "arabic": "وَلَنْ يَضِيْقَ رَسُوْلَ اللَّهِ جَاهُكَ بِي ",
        "arabic1":"إِذَا الْكَرِيْمُ تَجَلَّى بِاسْمِ مُنْتَقِمِ"
    },
    {
        "id": 154,
        "arabic": "فَإِنَّ مِنْ جُوْدِكَ الدُّنْيَا وَضَرَّتَهَا ",
        "arabic1":"وَمِنْ عُلُوْمِكَ عِلْمَ اللَّوْحِ وَالْقَلَمِ"
    },
    {
        "id": 155,
        "arabic": " يَا نَفْسُ لَا تَقْنَطِي مِنْ زَلَّةٍ عَظُمَتْ ",
        "arabic1":"إِنَّ الْكَبَائِرَ فِي الْغُفْرَانِ كَاللَّمَمِ"
    },
    {
        "id": 156,
        "arabic": "لَعَلَّ رَحْمَةَ رَبِّي حِيْنَ يَقْسِمُهَا ",
        "arabic1":"تَأْتِي عَلَى حَسَبِ الْعِصْيَانِ فِي الْقِسَمِ"
    },
    {
        "id": 157,
        "arabic": "يَا رَبِّ وَاجْعَلْ رَجَائِي غَيْرَ مُنْعَكِسٍ ",
        "arabic1":"لَدَيْكَ وَاجْعَلْ حِسَابِي غَيْرَ مُنْخَزِمِ"
    },
    {
        "id": 158,
        "arabic": " وَالْطُفْ بِعَبْدِكَ فِي الدَّارَيْنِ إِنَّ لَهُ",
        "arabic1":" صَبْرًا مَتَى تَدْعُهُ الْأَهْوَالِ يَنْهَزِمِ"
    },
    {
        "id": 159,
        "arabic": "وَأْذَنْ لِسُحْبِ صَلَاةٍ مِنْكَ دَائِمَةٍ ",
        "arabic1":"عَلَى النَّبِيِّ بِمُنْهَلٍّ وَمُنْسَجِمِ"
    },
    {
        "id": 160,
        "arabic": "وَالْآلِ وَالصَّحْبِ ثُمَّ التَّابِعِيْنَ فَهُمْ",
        "arabic1":" أَهْلُ التُّقَى وَالنَّقَا وَالْحِلْمِ وَالْكَرَمِ"
    },
